# generated by getup-engine

Host bastion
    Hostname                ${BASTION_ENDPOINT}
    ProxyCommand            none

Host master
    Hostname ${master1}

Host *
    IdentityFile            ~/.ssh/id_rsa
    ProxyCommand            ssh bastion nc %h %p 2> /dev/null
    User                    ${DEFAULT_USER}
    StrictHostKeyChecking   no
    CheckHostIP             no
    RequestTTY              yes
    ForwardAgent            yes
